{
  "name": "@ellucian/experience-extension-extras",
  "version": "0.5.0",
  "description": "Experience Extension extra Components and utilities",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.3.0"
  },
  "main": "prod/index.js",
  "files": [
    "dev",
    "prod"
  ],
  "scripts": {
    "xxbuild-dev": "webpack --progress --mode development --env verbose",
    "build": "run-s build-dev build-prod",
    "build-dev": "run-s clean-dev build-src-dev",
    "build-src-dev": "BABEL_ENV=development NODE_ENV=development ES6=true babel src/ --out-dir dev",
    "build-prod": "run-s clean-prod build-src-prod",
    "build-src-prod": "BABEL_ENV=production NODE_ENV=production ES6=true babel src/ --out-dir prod",
    "clean-dev": "rimraf ./dev",
    "clean-prod": "rimraf ./prod"
  },
  "dependencies": {
    "@ellucian/experience-extension-extras": "github:ellucian-developer/experience-extension-extras",
    "@tanstack/react-query": "^4.29.5",
    "dexie": "^3.2.4",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "loglevel": "^1.8.1",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "7.12.3",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/runtime": "7.12.1",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.32.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1"
  },
  "peerDependencies": {
    "@ellucian/experience-extension-utils": "https://cdn.elluciancloud.com/assets/SDK/utils/1.0.0/ellucian-experience-extension-utils-1.0.0.tgz",
    "prop-types": "15.7.2",
    "react": "17.0.2"
  }
}
